
<form style="text-align:left" method="get" action="/quizes">
		Buscador de preguntas:<p>
		<input type="text" name="search" value="Elementos a buscar"/>
		<input type="submit" value="Buscar"/>
</form>

<p style="text-align:left">
<% if (quizes!=""){ %>
<br>Lista de resultados:<br>
<% } %>
<table>
<% var i;for (i=0;i< quizes.length;i++){ %>
	<tr><td><a href="/quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a></td></tr>
<% } %>
</table>
</p>
